//2

#include<stdio.h>
main()
{
	char ch1,ch2,ch3;
	int flag = 0,flag2 = 0,flag1 = 0;
	ch1 = getchar();
	if(ch1!='\n')
	{
		ch2 = getchar();
		if(ch2!='\n')
		{
			ch3 = getchar();
	        if(ch1>'C'||ch1<'A'||ch2<'A'||ch2>'C')
	        flag1 = 1;
	        while(ch3!='\n'&&flag1 != 1&&flag2 != 1)
	        {
		        if(ch3>'C'||ch3<'A')
		        flag2 = 1;
		        else if(ch3!=ch2&&ch3!=ch1&&ch1!=ch2)
		        flag = 1;
		
		        ch1 = ch2;
		        ch2 = ch3;
	        	ch3 = getchar();
	        }      
		}
	
	}
	
	if(flag2 != 1)
	{
		if(flag == 1)
		printf("true\n");
		else
		printf("false\n");
	}
	else
	printf("false\n");
	
	return 0;
}

