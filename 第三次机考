	return 0;
}
void choice(int x)
{
	if(x%2 == 0&&x!=2)
	{
		choice((x-2)/2);
		printf("2");
	}
	else if(x%2 == 1&&x!=1)
	{
		choice((x-1)/2);
		printf("1");
	}
	else if(x == 1)
	printf("1");
	else if(x == 2)
	printf("2");
}*/


