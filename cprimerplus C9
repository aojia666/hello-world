/*#include<stdio.h>
int report(char);
main()
{
	char ch;
	scanf("%c",&ch);
	while(ch!='#')
	{
		printf("%d ",report(ch));
		scanf("%c",&ch);
	}
	
	return 0;
}
int report(char ch)
{
	if('a'<=ch&&'z'>=ch)
	return ch - 'a' + 1;
	else if('A'<=ch&&'Z'>=ch)
	return ch - 'A' + 1;
	else
	return -1;
}*/

