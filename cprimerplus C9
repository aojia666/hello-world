/*#include<stdio.h>
double power(double x,int exp);
main()
{
	int exp;
	double x;
	scanf("%lf%d",&x,&exp);
	printf("%lf",power(x,exp));
	
	return 0;
}
double power(double x,int exp)
{
	int i;
	double ans = 1;
	if(exp == 0)
	;
	else if(exp>0)
	{
		for(i = 1;i<=exp;i++)
		ans *= x;
	}
	else
	{
		for(i = 1;i<=-exp;i++)
		ans *= x;
		ans = 1/ans;
	}
	return ans;
}*/

//9

#include<stdio.h>
double power(double,int);
main()
{
	double num;
	int n;
	scanf("%lf%d",&num,&n);
	printf("%lf",power(num,n));
	
	return 0;
}
double power(double x,int i)
{
	if(i == 0)
	return 1;
	else if(i>0)
	return x*power(x,i-1);
	else if(i<0)
	return power(x,i+1)/x;
}

 
